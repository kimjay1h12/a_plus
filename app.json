{
  "cli": {
    "version": ">= 0.38.2"
  },
  "expo": {
    "name": "a_plus",
    "slug": "a_plus",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/img/newsplash.png",
      "resizeMode": "cover",
      "backgroundColor": "#407BFF"
    },
    "assetBundlePatterns": ["**/*"],
    "plugins": [
      [
        "expo-calendar",
        {
          "calendarPermission": "The app needs to access your calendar."
        }
      ]
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.kimjay.a_plus"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "extra": {
      "eas": {
        "projectId": "46164c68-8367-4122-98b7-28e2d69b7865"
      }
    },
    "runtimeVersion": {
      "policy": "sdkVersion"
    },
    "updates": {
      "url": "https://u.expo.dev/46164c68-8367-4122-98b7-28e2d69b7865"
    }
  }
}
